	.single-project .cs-post-pagination a.cs-post-next { float: right; color: #333; }
.cs-post-pagination a.cs-post-prev { color: #ffffff; font-size: 14px; text-align: left; /*display: inline-block;
    font-size: 14px;
    min-width: 48%;
